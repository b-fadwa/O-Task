	// TaskTags
Function generateTaskTags()
	var $tags : cs:C1710.TagSelection
	var $tag : cs:C1710.TagEntity
	var $tasks : cs:C1710.TaskSelection
	var $task : cs:C1710.TaskEntity
	var $taskTag : cs:C1710.TaskTagEntity
	var $info : Object
	
	$tags:=ds:C1482.Tag.all()
	$tasks:=ds:C1482.Task.all()
	
	For each ($task; $tasks)
		For each ($tag; $tags)
			If (Random:C100%3=0)
				$taskTag:=ds:C1482.TaskTag.new()
				$taskTag.task:=$task
				$taskTag.tag:=$tag
				$info:=$taskTag.save()
			End if 
		End for each 
	End for each 
	
	// Task Comments
Function generateComments()
	var $tasks : cs:C1710.TaskSelection
	var $task : cs:C1710.TaskEntity
	var $comment : cs:C1710.CommentEntity
	var $activity : cs:C1710.ActivityEntity
	var $users : cs:C1710.UserSelection
	var $info : Object
	var $randomNum; $index : Integer
	
	$tasks:=ds:C1482.Task.all()
	$users:=ds:C1482.User.all()
	
	For each ($task; $tasks)
		$randomNum:=Random:C100%10
		For ($index; 1; $randomNum+1)
			$comment:=ds:C1482.Comment.new()
			$comment.content:=This:C1470.sentences.at(Random:C100%This:C1470.sentences.length)
			$comment.createdAt:=Current date:C33()
			$comment.createdTime:=Current time:C178()
			$comment.task:=$task
			$comment.user:=$users.at(Random:C100%$users.length)
			$comment.order:=$task.comments.length
			$info:=$comment.save()
			This:C1470.createNotification("comment"; $comment.task.user; $comment.user.fullName+" has added a new comment to this task: "+$comment.task.name+"\" in the $board: \""+$comment.task.boardOfTask.name+"\"")
			$activity:=ds:C1482.Activity.new()
			$activity.user:=$comment.user
			$activity.createdAt:=Current date:C33()
			$activity.creationTime:=Current time:C178()
			$activity.content:="commented on this task: "+$comment.content
			$activity.subject:="task"
			$activity.problem:=Null:C1517
			$activity.task:=$task
			$info:=$activity.save()
			$task.reload()  // to get $task.comments.length 
		End for 
	End for each 
	
	// Notifications
Function createNotification($typeNotif : Text; $user : cs:C1710.UserEntity; $comment : Text)
	var $notification : cs:C1710.NotificationEntity
	var $info : Object
	$notification:=ds:C1482.Notification.new()
	$notification.content:=$comment
	$notification.createdAt:=Current date:C33()
	$notification.creationTime:=Current time:C178()
	$notification.isRead:=False:C215
	$notification.type:=$typeNotif
	$notification.content:=$comment
	$notification.user:=$user
	$info:=$notification.save()
	
	// Incidents
Function generateIncidents()
	var $colSeverity : Collection
	var $colReproducible : Collection
	var $incident : cs:C1710.IncidentEntity
	var $activity : cs:C1710.ActivityEntity
	var $users : cs:C1710.UserSelection
	var $i : Integer
	var $item : Text
	$i:=0
	$colSeverity:=["none"; "major"; "minor"; "critical"]
	$colReproducible:=["yes"; "no"; "sometimes"]
	$users:=ds:C1482.User.all()
	For each ($item; This:C1470.incidentTitles)
		$incident:=ds:C1482.Incident.new()
		$incident.title:=$item
		$incident.description:=This:C1470.incidentDescriptions.at($i)
		$incident.user:=$users.at(Random:C100%$users.length)
		$incident.createdAt:=Current date:C33()
