	// Tasks
Function generateTasks()
	var $tasks : Collection
	var $info : Object
	var $item : Text
	var $task : cs:C1710.TaskEntity
	var $activity : cs:C1710.ActivityEntity
	var $users : cs:C1710.UserSelection
	var $boards : cs:C1710.BoardSelection
	var $board : cs:C1710.BoardEntity
	var $listTODO : cs:C1710.ListEntity
	var $priority : Collection
	var $p : Integer
	$users:=ds:C1482.User.all()
	$boards:=ds:C1482.Board.all()
	$tasks:=This:C1470.words
	$priority:=["none"; "low"; "medium"; "high"]  // [0, 1, 2, 3]
	For each ($item; $tasks)
		$task:=ds:C1482.Task.new()
		$task.createdAt:=Current date:C33()
		$task.createdTime:=Current time:C178()
		$task.startDate:=Current date:C33()
		$task.endDate:=Add to date:C393(Current date:C33(); 0; 0; (Random:C100%(15+2+1))-2)  // for random, it's between -2 and 15
		$task.name:=$item
		$task.description:=This:C1470.sentences.at(Random:C100%This:C1470.sentences.length)
		$task.track:=(Random:C100%(25-5+1))+5
		$board:=$boards.at(Random:C100%$boards.length)
		$listTODO:=ds:C1482.List.query("name = :1 AND board.ID = :2"; "TODO"; $board.ID).first()
		$task.list:=$board.lists.at(Random:C100%$board.lists.length)
		$task.user:=($board.private) ? $board.user : $users.at(Random:C100%$users.length)
		$task.isImportant:=False:C215
		$p:=Random:C100%$priority.length
		Case of 
			: ($p=0)
				$task.priority:="none"
			: ($p=1)
				$task.priority:="low"
			: ($p=2)
				$task.priority:="medium"
			: ($p=3)
				$task.priority:="high"
		End case 
		$task.createdBy:=$task.user
		If ($task.list.name="Closed")
			$task.list:=$listTODO
		End if 
		$info:=$task.save()
		This:C1470.createNotification("task"; $task.user; "New task has been assigned to you :\""+$task.name+"\" in the board: \""+$board.name+"\"")
		$activity:=ds:C1482.Activity.new()
		$activity.user:=$task.user
		$activity.createdAt:=Current date:C33()
		$activity.creationTime:=Current time:C178()
		$activity.content:="created the task"
		$activity.subject:="task"
		$activity.problem:=Null:C1517
		$activity.task:=$task
		$info:=$activity.save()
	End for each 
	
	// Boards
Function generateBoards()
	var $boards : Collection
	var $item : Text
	var $info : Object
	var $board : cs:C1710.BoardEntity
	var $list : cs:C1710.ListEntity
	var $users : cs:C1710.UserSelection
	var $zone : Collection
	var $p : Integer
	$boards:=["WrkPass"; "O'Task"; "KnowBase"; "Event Manager"; "Formation"; "Restaurant Manager"]
	$zone:=["us"; "germany"; "france"; "intl"]  // [0, 1, 2, 3]
	$users:=ds:C1482.User.all()
	
	For each ($item; $boards)
		$board:=ds:C1482.Board.new()
		$board.name:=$item
		$board.description:=This:C1470.sentences.at(Random:C100%This:C1470.sentences.length)
		$board.createdAt:=Current date:C33()
		$board.private:=(Random:C100%(1-0+1)+0=1)
		$board.user:=$users.at(Random:C100%$users.length)
		$p:=Random:C100%$zone.length
		Case of 
			: ($p=0)
				$board.zone:="us"
			: ($p=1)
				$board.zone:="germany"
			: ($p=2)
				$board.zone:="france"
			: ($p=3)
				$board.zone:="intl"
		End case 
		$info:=$board.save()
		$list:=ds:C1482.List.new()
		$list.board:=$board
		$list.name:="Closed"
		$list.save()
	End for each 
	
	// List
Function generateList()
	var $lists : Collection
	var $item : Text
	var $boards : cs:C1710.BoardSelection
	var $board : cs:C1710.BoardEntity
	var $list : cs:C1710.ListEntity
	var $info : Object
	
	$lists:=["TODO"; "In Progress"; "Done"]
	$boards:=ds:C1482.Board.all()
	For each ($board; $boards)
		For each ($item; $lists)
			$list:=ds:C1482.List.new()
			$list.name:=$item
			$list.board:=$board
			$info:=$list.save()
		End for each 
	End for each 
	
